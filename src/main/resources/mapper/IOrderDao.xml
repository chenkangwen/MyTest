<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.IOrderDao">

    <select id="getOrderById" resultType="com.example.demo.entity.Order">
        select * from t_order where id=#{id}
    </select>

    <insert id="insertOrder">
        insert into t_order
        (
          id,order_no, order_time,creater,order_content
        )
        values
        (
          #{id}, #{orderNo},#{orderTime},#{creater},#{orderContent}
        )
    </insert>


    <select id="getOrderList" resultType="com.example.demo.entity.Order">
        select * from t_order
    </select>
</mapper>