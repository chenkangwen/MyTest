<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.IUserDAO">


    <insert id="batchInsert" parameterType="com.example.demo.entity.UserDTO">
        insert into t_user
        (
        user_id,
        role_id,
        name
        )VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (
            #{item.userId},
            #{item.roleId},
            #{item.name}
            )
        </foreach>
    </insert>

    <insert id="insertUserDTO" parameterType="com.example.demo.entity.UserDTO">
        insert into t_user
        (
        user_id,
        role_id,
        name
        )VALUES
         (
          #{userId},
          #{roleId},
          #{name}
         )
    </insert>

</mapper>